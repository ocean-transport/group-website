	Doi = {10.1029/2019GL083074},
	Eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2019GL083074},
	Journal = {Geophysical Research Letters},
	Keywords = {LLC4320, surface drifter, rotary spectrum, SWOT},
	Number = {16},
